<p id="notice"><%= notice %></p>

<h1>Coinbags</h1>

<table>
  <thead>
    <tr>
      <th>Coinbag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coinbags.each do |coinbag| %>
      <tr>
        <td><%= coinbag.coinbag %></td>
        <td><%= link_to 'Show', coinbag %></td>
        <td><%= link_to 'Edit', edit_coinbag_path(coinbag) %></td>
        <td><%= link_to 'Destroy', coinbag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coinbag', new_coinbag_path %>
